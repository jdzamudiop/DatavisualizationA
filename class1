library(tidyverse)
library(generics)
library(estimatr)

raw<-read.csv("hh_98.csv")
hist(raw$agehead)
summary(raw)
head(raw)


#CDF
ggplot(raw,aes(x=exptot,color=factor(sexhead)))+#make campus
  stat_ecdf() #Make histogram

ggplot(raw,aes(x=exptot))+#make campus
  geom_histogram()+#make histogram
  facet_wrap(~sexhead)
#microfinance use 

